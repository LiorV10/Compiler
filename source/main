struct Match* MakeMatch(char *start, char *end, unsigned short matchType)
{
    char *ptr;
    struct Match *match = malloc(sizeof(struct Match));

    match->start = malloc(sizeof(char) * (end - start));
    ptr = match->start;

    for (; start != end;)
    {
        *ptr++ = *start++;
    }

    *ptr ^= *ptr;
    match->end = ptr;
    match->matchType = --matchType;
}

