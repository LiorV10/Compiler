unsigned char Exists(CircularLinearLinkedListNode *l, void *v)
{
    if (!l) return 0;

    CircularLinearLinkedListNode *p = l;
    do
    {
        if (p->info == v) return 1;

        p = p->nextNode;
    }
    while (p != l);

    return 0;
}

